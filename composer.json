{
    "name": "nvahalik/laravel-phalcon",
    "description": "Laravel to Phalcon bridge",
    "keywords": [
        "Nick Vahalik",
        "laravel",
        "laravel-phalcon"
    ],
    "homepage": "https://github.com/nvahalik/laravel-phalcon",
    "license": "MIT",
    "authors": [
        {
            "name": "Nick Vahalik",
            "email": "nick@nickvahalik.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3",
        "ext-phalcon": "^5.9",
        "illuminate/contracts": "^11.0||^12.0",
        "phalcon/ide-stubs": "^5.9",
        "spatie/laravel-package-tools": "^1.16"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.8",
        "larastan/larastan": "^3.0",
        "orchestra/testbench": "^10.0.0||^9.0.0",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-arch": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Vahalik\\LaravelPhalcon\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vahalik\\LaravelPhalcon\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vahalik\\LaravelPhalcon\\LaravelPhalconServiceProvider"
            ],
            "aliases": {
                "LaravelPhalcon": "Vahalik\\LaravelPhalcon\\Facades\\LaravelPhalcon"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
